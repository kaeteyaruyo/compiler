Grammar

    0 $accept: translation_unit $end

    1 translation_unit: external_declaration
    2                 | translation_unit external_declaration

    3 external_declaration: variable_declaration
    4                     | function_declaration ';'
    5                     | function_definition compound_statement

    6 variable_declaration: type_specifier ID ';'
    7                     | type_specifier ID '=' assignment_expression ';'

    8 function_declaration: type_specifier ID '(' ')'
    9                     | type_specifier ID '(' parameter_list ')'

   10 function_definition: type_specifier ID '(' ')'
   11                    | type_specifier ID '(' parameter_list ')'

   12 type_specifier: VOID
   13               | INT
   14               | FLOAT
   15               | STRING
   16               | BOOL

   17 parameter_list: parameter_declarator
   18               | parameter_list ',' parameter_declarator

   19 parameter_declarator: type_specifier ID

   20 compound_statement: '{' '}'
   21                   | '{' block_item_list '}'

   22 block_item_list: block_item
   23                | block_item_list block_item

   24 block_item: variable_declaration
   25           | statement

   26 assignment_expression: unary_expression assignment_operator assignment_expression
   27                      | conditional_expression

   28 unary_expression: postfix_expression
   29                 | INC_OP unary_expression
   30                 | DEC_OP unary_expression
   31                 | unary_operator unary_expression

   32 postfix_expression: primary_expression
   33                   | ID '(' ')'
   34                   | ID '(' argument_expression_list ')'
   35                   | postfix_expression INC_OP
   36                   | postfix_expression DEC_OP

   37 primary_expression: ID
   38                   | constant
   39                   | '(' expression ')'

   40 constant: I_CONST
   41         | F_CONST
   42         | TRUE
   43         | FALSE
   44         | STRING_LITERAL

   45 expression: assignment_expression
   46           | expression ',' assignment_expression

   47 argument_expression_list: assignment_expression
   48                         | argument_expression_list ',' assignment_expression

   49 unary_operator: '+'
   50               | '-'
   51               | '!'

   52 assignment_operator: '='
   53                    | MUL_ASSIGN
   54                    | DIV_ASSIGN
   55                    | MOD_ASSIGN
   56                    | ADD_ASSIGN
   57                    | SUB_ASSIGN

   58 conditional_expression: logical_or_expression
   59                       | logical_or_expression '?' expression ':' conditional_expression

   60 logical_or_expression: logical_and_expression
   61                      | logical_or_expression OR_OP logical_and_expression

   62 logical_and_expression: equality_expression
   63                       | logical_and_expression AND_OP equality_expression

   64 equality_expression: relational_expression
   65                    | equality_expression EQ_OP relational_expression
   66                    | equality_expression NE_OP relational_expression

   67 relational_expression: additive_expression
   68                      | relational_expression '<' additive_expression
   69                      | relational_expression '>' additive_expression
   70                      | relational_expression LE_OP additive_expression
   71                      | relational_expression GE_OP additive_expression

   72 additive_expression: multiplicative_expression
   73                    | additive_expression '+' multiplicative_expression
   74                    | additive_expression '-' multiplicative_expression

   75 multiplicative_expression: unary_expression
   76                          | multiplicative_expression '*' unary_expression
   77                          | multiplicative_expression '/' unary_expression
   78                          | multiplicative_expression '%' unary_expression

   79 statement: compound_statement
   80          | expression_statement
   81          | selection_statement
   82          | iteration_statement
   83          | jump_statement
   84          | print_statement

   85 expression_statement: ';'
   86                     | expression ';'

   87 selection_statement: IF '(' expression ')' statement
   88                    | IF '(' expression ')' statement ELSE statement

   89 iteration_statement: WHILE '(' expression ')' statement
   90                    | FOR '(' expression_statement expression_statement ')' statement
   91                    | FOR '(' expression_statement expression_statement expression ')' statement
   92                    | FOR '(' variable_declaration expression_statement ')' statement
   93                    | FOR '(' variable_declaration expression_statement expression ')' statement

   94 jump_statement: RETURN ';'
   95               | RETURN expression ';'

   96 print_statement: PRINT '(' ID ')' ';'
   97                | PRINT '(' STRING_LITERAL ')' ';'


