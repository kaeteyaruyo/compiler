Grammar

    0 $accept: translation_unit $end

    1 translation_unit: external_declaration
    2                 | translation_unit external_declaration

    3 external_declaration: variable_declaration
    4                     | function_declarator ';'
    5                     | function_declarator compound_statement

    6 variable_declaration: type_specifier ID ';'
    7                     | type_specifier ID '=' assignment_expression ';'

    8 function_declarator: type_specifier ID '(' ')'
    9                    | type_specifier ID '(' parameter_list ')'

   10 type_specifier: VOID
   11               | INT
   12               | FLOAT
   13               | STRING
   14               | BOOL

   15 parameter_list: parameter_declarator
   16               | parameter_list ',' parameter_declarator

   17 parameter_declarator: type_specifier ID

   18 compound_statement: '{' '}'
   19                   | '{' block_item_list '}'

   20 block_item_list: block_item
   21                | block_item_list block_item

   22 block_item: variable_declaration
   23           | statement

   24 assignment_expression: unary_expression assignment_operator assignment_expression
   25                      | conditional_expression

   26 unary_expression: postfix_expression
   27                 | INC_OP unary_expression
   28                 | DEC_OP unary_expression
   29                 | unary_operator unary_expression

   30 postfix_expression: primary_expression
   31                   | ID '(' ')'
   32                   | ID '(' argument_expression_list ')'
   33                   | postfix_expression INC_OP
   34                   | postfix_expression DEC_OP

   35 primary_expression: ID
   36                   | constant
   37                   | '(' expression ')'

   38 constant: I_CONST
   39         | F_CONST
   40         | TRUE
   41         | FALSE
   42         | STRING_LITERAL

   43 expression: assignment_expression
   44           | expression ',' assignment_expression

   45 argument_expression_list: assignment_expression
   46                         | argument_expression_list ',' assignment_expression

   47 unary_operator: '+'
   48               | '-'
   49               | '!'

   50 assignment_operator: '='
   51                    | MUL_ASSIGN
   52                    | DIV_ASSIGN
   53                    | MOD_ASSIGN
   54                    | ADD_ASSIGN
   55                    | SUB_ASSIGN

   56 conditional_expression: logical_or_expression
   57                       | logical_or_expression '?' expression ':' conditional_expression

   58 logical_or_expression: logical_and_expression
   59                      | logical_or_expression OR_OP logical_and_expression

   60 logical_and_expression: equality_expression
   61                       | logical_and_expression AND_OP equality_expression

   62 equality_expression: relational_expression
   63                    | equality_expression EQ_OP relational_expression
   64                    | equality_expression NE_OP relational_expression

   65 relational_expression: additive_expression
   66                      | relational_expression '<' additive_expression
   67                      | relational_expression '>' additive_expression
   68                      | relational_expression LE_OP additive_expression
   69                      | relational_expression GE_OP additive_expression

   70 additive_expression: multiplicative_expression
   71                    | additive_expression '+' multiplicative_expression
   72                    | additive_expression '-' multiplicative_expression

   73 multiplicative_expression: unary_expression
   74                          | multiplicative_expression '*' unary_expression
   75                          | multiplicative_expression '/' unary_expression
   76                          | multiplicative_expression '%' unary_expression

   77 statement: compound_statement
   78          | expression_statement
   79          | selection_statement
   80          | iteration_statement
   81          | jump_statement
   82          | print_statement

   83 expression_statement: ';'
   84                     | expression ';'

   85 selection_statement: IF '(' expression ')' statement
   86                    | IF '(' expression ')' statement ELSE statement

   87 iteration_statement: WHILE '(' expression ')' statement
   88                    | FOR '(' expression_statement expression_statement ')' statement
   89                    | FOR '(' expression_statement expression_statement expression ')' statement
   90                    | FOR '(' variable_declaration expression_statement ')' statement
   91                    | FOR '(' variable_declaration expression_statement expression ')' statement

   92 jump_statement: RETURN ';'
   93               | RETURN expression ';'

   94 print_statement: PRINT '(' ID ')' ';'
   95                | PRINT '(' STRING_LITERAL ')' ';'


